/////////////////////////////////////////////////////
bool is_isogram(std::string str) {
  int len = str.length(); char s;
  for (int i = 0; i < (len-1); ++i){
    s = std::tolower(str[i]);
    for (int j = i+1; j < len; ++j){
        if (std::tolower(str[j]) == s) return false;
    }
  }
  return true;
}
